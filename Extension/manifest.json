{
    "manifest_version": 3,
    "name": "RAG Extension",
    "version": "1.0",
    "description": "An extension to extract text data from webpages.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],  // To access all URLs for content extraction
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  